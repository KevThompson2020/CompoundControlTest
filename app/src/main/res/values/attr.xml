<resources>

    <attr name="title" format="string" />
    <attr name="hint" format="string" />

    <!--SideSpinner-->
    <declare-styleable name="SideSpinner">
        <attr name="values" format="reference" />

        <!-- any custom attributes -->
        <attr name="title" />
    </declare-styleable>


    <!--HAPI form -->
    <declare-styleable name="HapiTextEntry">
        <attr name="hint"/>
        <attr name="warning_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="HapiSubHeader">
        <attr name="title"/>
    </declare-styleable>

</resources>